<div class="row">
  <div class="col-lg-12">
      <div class="panel panel-default">
          <div class="panel-heading">
              Alimentador {{model.id}}
          </div>
          <div class="panel-body">
              <div class="row">
                  <div class="col-lg-6">
                          <div class="form-group">
                              <label>Alimentador</label>
                              {{model.id}}
                          </div>
                          <div class="form-group">
                              <label>Nome</label>
                              {{model.nickname}}
                          </div>
                          <div class="form-group">
                              <label>Tanque</label>
                              {{model.tank.name}}
                          </div>
                          <div class="form-group">
                              <label>Nivel do tanque</label>
                              {{parse-nivel model.nivel}}
                          </div>
            </div>
        </div>
      </div>
    </div>
  </div>
</div>
